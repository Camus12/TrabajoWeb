<h1>Actualizar Datos</h1>
<%= form_for :Actualizar, :html =>{ :method => :get, :class => "form-horizontal"} do |f|	%>
<div class ="form-group">
		<div class = "col-sm-2">
			<%=f.label :Nombre %>
		 </div>
	
		<div class ="col-sm-8">
		 	<%=f.text_field :Nombre, { :class => " form-control", :placeholder => "Ingrese Nombre"} %>
		 </div>
		 <div class = "col-sm-2">
		 </div>
	 </div> 

	 <div class ="form-group">
			<div class ="col-sm-2">
			<%=f.label :ApellidoP,"Apellido Paterno" %> 
			</div>
			<div class ="col-sm-8">
		 	<%=f.text_field :ApellidoP, { :class => " form-control", :placeholder => "Ingrese Apellido Paterno"} %>
			 </div>
			  <div class = "col-sm-2">
		 </div>

	 </div> 

	  <div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :ApellidoM,"Apellido Materno" %> 
		</div>
		<div class ="col-sm-8">
		 	<%=f.text_field :ApellidoM, { :class => " form-control", :placeholder => "Ingrese Apellido Materno"} %>
		 </div>
		  <div class = "col-sm-2">
		 </div>

	</div> 
  	<div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :Sexo,"Sexo" %> 
		</div>
		<div class="col-sm-10">
			<label class="radio-inline"><input type="radio" name="Tipo" checked>Hombre</label>
			<label class="radio-inline"><input type="radio" name="Tipo" checked>Mujer</label>

		</div> 
	</div> 

	<div class ="form-group">
		<div class ="col-sm-2">
			<%=f.label :Tipo_Doc,"Tipo de Documento" %>
		</div>
		<div class ="col-sm-4">
			<%= f.select :Tipo_Doc, [["DNI",1],["Pasaporte",2],["CarneExtra",3]], { :include_blank => "Seleccione Tipo Doc"},{:class => "form-control"} %>
		</div>

		
		<div class="col-sm-4">
			<%=f.text_field :Num_Doc,{ :class => "form-control", :placeholder =>"Ingrese Numero de Documento"} %>
		</div>

	</div>

		<div class ="form-group">
		<div class ="col-sm-2">
			<%=f.label :Distrito,"Distrito" %>
		</div>
		<div class ="col-sm-4">
			<%= f.select :Tipo_Doc, [["San Luis",1],["San Borja",2],["Surco",3]], { :include_blank => "Seleccione Distrito"},{:class => "form-control"} %>
		</div>

		
		

	</div>


	  <div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :Dirreccion,"Dirreccion" %> 
		</div>
		<div class ="col-sm-8">
		 	<%=f.text_field :Dirreccion, { :class => " form-control", :placeholder => "Ingrese Dirreccion"} %>
		 </div>
		  <div class = "col-sm-2">
		 </div>

	</div> 


	  <div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :Correoconf," Correo" %> 
		</div>
		<div class ="col-sm-8">
		 	<%=f.text_field :Correo, { :class => " form-control", :placeholder => "Ingrese Correo"} %>
		 </div>
		  <div class = "col-sm-2">
		 </div>

	</div> 


		  <div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :Contrase単a,"Contrase単a" %> 
		</div>
		<div class ="col-sm-8">
		 	<%=f.text_field :Contrase単a, { :class => " form-control", :placeholder => "Ingrese Contrase単a"} %>
		 </div>
		  <div class = "col-sm-2">
		 </div>

	</div> 


		  <div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :Celular,"Ingrese Telefono" %> 
		</div>
		<div class ="col-sm-8">
		 	<%=f.text_field :Celular, { :class => " form-control", :placeholder => "Ingrese Telefono"} %>
		 </div>
		  <div class = "col-sm-2">
		 </div>

	</div> 

	<div class ="form-group">
		<div class ="col-sm-2">
		<%=f.label :Fecha,"Fecha Nacimiento" %> 
		</div>
		
   
        <div class="col-sm-6">
        <div class="input-append date" id="dp3" data-date="01-01-2019" data-date-format="dd-mm-yyyy">
  <input class="span2" size="16" type="text" value="01-01-2019">
  <span class="add-on"><i class="icon-th"></i></span>
</div>
    </div>

	</div>

	<div class ="form-group">
		<div class="col-sm-2">
		<%=f.submit "Cancelar", :class => "btn btn-danger"%>
		</div>

		<div class="col-sm-10">
			<%=f.submit "Guardar", :class => "btn btn-primary"%>
		</div>

</div>



<% end %>